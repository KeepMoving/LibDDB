<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">


<hibernate-mapping 	package="com.lib.project.model">
	<class name="ReaderType" table="readertype">
		<id name="id" column="id" type="java.lang.Integer"/>
		<property name="jbType" column="jbType" type="string"/>
		<property name="timeLimit" column="timeLimit" type="int"/>
		<property name="quantityLimit" column="quantityLimit" type="int"/>	
	</class>
</hibernate-mapping>
