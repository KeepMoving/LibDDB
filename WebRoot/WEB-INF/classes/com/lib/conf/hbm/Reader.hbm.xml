<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">


<hibernate-mapping 	package="com.lib.project.model"> 
	<class name="Reader" table="reader">
		<id name="readerID" column="readerID" />
   		<property name="typeID" column="typeID" type="java.lang.Integer"/>
		<property name="cardType" column="cardType" type="string"/>
		<property name="cardNo" column="cardNo" type="string"/>	
		<property name="operator" column="operator" type="java.lang.Integer"/>	
		<property name="createDate" column="createDate" type="java.sql.Timestamp"/>	
   	    <property name="barCode" column="barCode" type="string"/>	
		<property name="readerName" column="readerName" type="string"/>	
		<property name="sex" column="sex" type="string"/>	
		<property name="tel" column="tel" type="string"/>	
		<property name="email" column="email" type="string"/>	
		<property name="password" column="password" type="string"/>	
	</class>
</hibernate-mapping>
